{
  "name": "vida-ui",
  "version": "1.0.0",
  "description": "Vida Lisboa design-system",
  "main": "index.js",
  "engines": {
    "yarn": ">=1.5.1",
    "node": ">=8.0.0"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "test": "jest",
    "fix-code": "prettier-eslint --write 'packages/**/*.{js,jsx}' ",
